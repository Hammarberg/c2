	"templates":[
		"6502":{
			"description" : "Plain 6502 for embedded projects",
			"super" : "c2i",
			"include" : "c2/h/c2i.h",
			"arguments" : "--address-range 0x0 0x10000",
			"output" : "--out - + {title}.bin",
			"source":[
				"c2i.cpp":{
					"flags" : "-O2"
				}
			],
			"template":[
				"asm.cpp":{
					"target" : "{source}.cpp"
					"c2" : true
					"flags" : "-O0"
				},
				"6502.s":{
					"target" : "{source}.s"
				}
			]
		},
